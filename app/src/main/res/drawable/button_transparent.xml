<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <stroke
                android:width="1dp"
                android:color="#fff"/>
            <corners
                android:radius="20dp"/>
            <solid
                android:color="#23ffffff"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <stroke
                android:width="0dp"
                android:color="#ffd7d6d6"/>
            <corners
                android:radius="20dp"/>
            <solid
                android:color="#23ffffff"/>
        </shape>
    </item>

</selector>